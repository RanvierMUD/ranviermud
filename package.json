    "joi": "^10.2.2",
    "js-yaml": "",
    "left-pad": "^1.1.3",
    "longjohn": "^0.2.11",
    "node-uuid": "",
    "optimist": "",
    "pretty-error": "^2.0.2",
    "purdy": "^2.2.0",
    "rando-js": "^0.1.1",
